        "# -----------------------------------------------------------\n",
        "# Part 3: Compute Statistics\n",
        "# ----------------------------------------------------------\n",
        "num_cols = [\"price\", \"area\", \"bedrooms\"]\n",
        "\n",
        "stats = {}\n",
        "\n",
        "for col in num_cols:\n",
        "    stats[col] = {\n",
        "        \"mean\": df[col].mean(),\n",
        "        \"median\": df[col].median(),\n",
        "        \"mode\": df[col].mode()[0],\n",
        "        \"variance\": df[col].var(),\n",
        "        \"std_dev\": df[col].std(),\n",
        "        \"skew\": df[col].skew(),\n",
        "        \"kurtosis\": df[col].kurt(),\n",
        "        \"min\": df[col].min(),\n",
        "        \"max\": df[col].max()\n",
        "    }\n",
        "\n",
        "print(\"\\nStatistical Summary:\")\n",
        "for col, values in stats.items():\n",
        "    print(f\"\\n--- {col.upper()} ---\")\n",
        "    for stat_name, stat_val in values.items():\n",
        "        print(f\"{stat_name}: {stat_val}\")"
      ],
